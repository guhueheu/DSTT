   "source": [
    "from sympy import Symbol\n",
    "\n",
    "x = Symbol('x')\n",
    "y = Symbol('y')\n",
    "bieuthuc = x**2 - x*y + y**2\n",
    "print(\"Biểu thức gốc:\", bieuthuc) \n",
    "\n",
    "giatri = bieuthuc.subs({x: 3, y: 2})\n",
    "print(\"Thay x=3, y=2:\", giatri)  \n",
    "\n",
    "print(\"x sau khi thay:\", x)  \n",
    "print(\"y sau khi thay:\", y) \n"
   ]
  }
 ],
