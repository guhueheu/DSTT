   "source": [
    "from sympy import Symbol, solve\n",
    "\n",
    "x = Symbol('x')\n",
    "y = Symbol('y')\n",
    "\n",
    "pt1 = 2*x + 3*y - 12\n",
    "pt2 = 3*x - 2*y - 5\n",
    "\n",
    "nghiem = solve((pt1, pt2), dict=True)\n",
    "print(\"Nghiệm của hệ phương trình là:\")\n",
    "print(nghiem)\n"
   ]
  }
