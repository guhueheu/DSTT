   "source": [
    "import numpy as np\n",
    "\n",
    "m = 10\n",
    "\n",
    "v1 = np.array([5, -4, 3])\n",
    "v2 = np.array([4, 3, -2])\n",
    "v3 = np.array([-4, -3, -1])\n",
    "v4 = np.array([-9, 8, 6])\n",
    "\n",
    "mi = np.array([2, 2, 2, 4])\n",
    "\n",
    "M = np.array([v1, v2, v3, v4])\n",
    "\n",
    "MT = M.transpose()\n",
    "\n",
    "v = (1.0 / m) * MT.dot(mi)\n",
    "\n",
    "print(\"Điểm trọng tâm là:\", v)\n"
   ]
  }
 ],
