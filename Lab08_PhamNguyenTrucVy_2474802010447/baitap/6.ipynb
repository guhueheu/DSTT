   "source": [
    "import sympy as sp\n",
    "\n",
    "c1, c2, c3 = sp.symbols('c1 c2 c3')\n",
    "\n",
    "eq1 = c1*1 + c2*0 + c3*(-2)   \n",
    "eq2 = c1*2 + c2*1 + c3*0    \n",
    "eq3 = c1*3 + c2*2 + c3*1    \n",
    "\n",
    "solution = sp.solve([eq1, eq2, eq3], (c1, c2, c3))\n",
    "\n",
    "print(\"Nghiệm của hệ phương trình là:\")\n",
    "print(solution)\n"
   ]
  }
