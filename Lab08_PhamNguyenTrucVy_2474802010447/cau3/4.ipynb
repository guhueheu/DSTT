      "Nghiệm không tầm thường Ker(f):\n",
      "{x1: 2*x3, x2: -x3}\n"
     ]
    }
   ],
   "source": [
    "import sympy as sp\n",
    "\n",
    "x1, x2, x3 = sp.symbols('x1 x2 x3')\n",
    "\n",
    "eq1 = x1 + x2 - x3\n",
    "eq2 = 2*x1 + 3*x2 - x3\n",
    "eq3 = 3*x1 + 5*x2 - x3\n",
    "\n",
    "sol = sp.solve([eq1, eq2, eq3], (x1, x2, x3))\n",
    "print(\"Nghiệm không tầm thường Ker(f):\")\n",
    "print(sol)\n"
   ]
  }
